{
	"name": "vscode-git-branch-compare",
	"displayName": "Git Branch Compare",
	"description": "Compare your worktree against a base branch",
	"version": "0.0.1",
	"publisher": "Maik Riechert",
	"engines": {
		"vscode": "^1.13.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:gitBranchCompare"
	],
	"main": "./out/src/extension",
	"contributes": {
		"views": {
			"explorer": [
				{
					"id": "gitBranchCompare",
					"name": "Branch Compare"
				}
			]
		},
		"commands": [
			{
				"command": "gitBranchCompare.diffWithBase",
				"title": "Compare File with Base",
				"category": "Branch Compare"
			},
			{
				"command": "gitBranchCompare.changeBase",
				"title": "Change Base...",
				"category": "Branch Compare"
			}
		],
		"menus": {
			"view/item/context": [
				{
					"command": "gitBranchCompare.diffWithBase",
					"when": "view == gitBranchCompare && viewItem == file"
				},
				{
					"command": "gitBranchCompare.changeBase",
					"when": "view == gitBranchCompare && viewItem == branch"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"typescript": "^2.3.4",
		"vscode": "^1.1.0",
		"@types/node": "*"
	},
	"dependencies": {
		"iconv-lite": "^0.4.15",
		"vscode-nls": "^2.0.1"
	}
}